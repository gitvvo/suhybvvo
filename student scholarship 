def check_scholarship():
    # Get student information
    grade = float(input("Enter student's grade (0-100): "))
    attendance = float(input("Enter attendance percentage (0-100): "))
    
    # Check eligibility criteria
    if grade >= 90 and attendance >= 95:
        print("Eligible for Gold Scholarship!")
    elif grade >= 80 and attendance >= 90:
        print("Eligible for Silver Scholarship!")
    elif grade >= 70 and attendance >= 85:
        print("Eligible for Bronze Scholarship!")
    else:
        print("Not eligible for any scholarship.")

# Call the function
check_scholarship()